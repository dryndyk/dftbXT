Geometry = xyzFormat {
 <<< "caffeine.xyz"
}

Driver {}

Hamiltonian = DFTB {
  SCC = Yes
  SccTolerance = 1e-10
  MaxAngularMomentum {
    H = "s"
    O = "p"
    C = "p"
    N = "p"
  }
  SlaterKosterFiles = Type2FileNames {
    Separator = "-"
    Suffix = ".skf"
  }
}

Options {
  WriteAutotestTag = Yes
}

Analysis {
  CalculateForces = Yes
}

ParserOptions {
  ParserVersion = 8
}
